{
  "name": "express-react-ssr",
  "version": "0.0.1",
  "description": "Boilerplate for Express, React, and SSR",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "clean": "rm -rf dist; rm -rf public/client",
    "babel": "babel src --out-dir dist",
    "webpack": "NODE_ENV=production webpack --config src/config/webpack.config.prod.babel.js -p",
    "dev": "NODE_ENV=development babel-node src",
    "prestart": "npm run babel; npm run webpack",
    "start": "NODE_ENV=production node dist"
  },
  "author": "Evan Corl",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "path": "^0.12.7",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^2.4.0"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-router": "^4.0.0-alpha.6",
    "react-router-addons-routes": "0.0.3",
    "serialize-javascript": "^1.3.0"
  }
}
