{
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  },
  "parserOptions": {
    "ecmaVersion": 2015,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "document": true,
    "window": true
  }
}
